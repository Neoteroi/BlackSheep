[flake8]
ignore = E203, E266, W503
max-line-length = 88
max-complexity = 18
per-file-ignores =
    neoteroi/web/__init__.py:F401
    neoteroi/web/client/__init__.py:F401
    neoteroi/web/server/__init__.py:F401
    tests/*:E501
    itests/*:E501

exclude =
    env
    .env
    venv
    venv311
    build
    .eggs
